<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lty.fsb.dao.daoDb1.user.UserMapper">
    <insert id="insert" parameterType="com.lty.fsb.entity.user.User">
        insert into user (user_name,password,sex,phone_num) values (#{userName},#{password},#{sex},#{phoneNum})
    </insert>
    <select id="selectAll" resultType="com.lty.fsb.entity.user.User">
    select * from user
</select>


</mapper>
